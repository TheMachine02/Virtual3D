#define	VX_PIXEL_SHADER_CODE		$E10010
#define	VX_PIXEL_SHADER_SIZE		64
#define	VX_PIXEL_SHADER_CONSTANT	$D00000
#define	VX_PIXEL_SHADER_SUBSIZE	10

#define	VX_REGISTER_SIZE			12
#define	VX_REGISTER0			0
#define	VX_REGISTER1			3
#define	VX_REGISTER2			6
#define	VX_REGISTER3			9

; locate write area as vxPixelShader + SUB_SIZE - 1, 1 byte to write
; for killing pixel, do a jr cc,$+3 right before ldi


VX_PIXEL_SHADER_COPY:

.relocate	VX_PIXEL_SHADER_CODE

vxPixelShader:
	ld	a, h
	add	hl, sp
	exx
	ld	h, a
;	ld	c, (hl)
	ld	a, (hl)
	adc	hl, de
;	ld	a, c
;	cp	$FF
;	ld	a, (bc)
	exx
;	jr	z, $+3
	ld	(de), a
	inc	de
	ld	a, h
	add	hl, sp
	exx
	ld	h, a
;	ld	c, (hl)
	ld	a, (hl)
	adc	hl, de
;	ld	a, c
;	cp	$FF
;	ld	a, (bc)
	exx
;	jr	z, $+3
	ld	(de), a
	inc	de
	djnz	vxPixelShader
vxShaderGeneralInterpolation:
	ld	hl, (iy+VX_REGISTER1)
	ld	de, (iy+VX_REGISTER0)
	add	hl, de
	add	hl, hl
	ld	a, (hl)
	ld	ixl, a
	inc	hl
	ld	b, (hl)
	ld	hl, (iy+VX_REGISTER2)	; v
	exx
	ld	hl, (iy+VX_REGISTER3)	; u
	exx
	lea	iy, iy+VX_REGISTER_SIZE
	jp	(ix)
	jp	vxPixelShaderExit
; exactly 64 bytes

.endrelocate

