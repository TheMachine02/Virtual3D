#define	VX_CALL1_POS	(VX_PIXEL_SHADER_SUBSIZE-1+VX_PIXEL_SHADER_CODE)%256
#define	VX_CALL_NULL	((VX_PIXEL_SHADER_SUBSIZE+1)*2+VX_PIXEL_SHADER_CODE)%256
